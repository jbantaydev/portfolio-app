{
  "functions": {
    "api/**/*.{js,ts}": {
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "prisma/schema.prisma"
    }
  },
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://portfolio.jbantay.work/$1",
      "permanent": true
    }
  ]
}
